{
  "name": "Magic Dictionary",
  "version": "2.1",
  "manifest_version": 2,
  "description": "Translates from slovak to various other languages.",
  "permissions":
  [
    "<all_urls>"
  ],
  "content_scripts":
  [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js","runner.js"],
      "css": ["magician.css"],
      "run_at": "document_end"
    }
  ]
}
